{
	"$schema":"https://docs.renovatebot.com/renovate-schema.json",
	"lockFileMaintenance":{
	   "enabled":true,
	   "automerge":true
	},
	"labels":[
	   "dependencies"
	],
	"prHourlyLimit":2,
	"prConcurrentLimit":5,
	"timezone":"America/Los_Angeles",
	"semanticCommitType":"chore",
	"packageRules":[
	   {
		  "matchUpdateTypes":[
			 "minor",
			 "patch"
		  ],
		  "matchCurrentVersion":"!/^0/",
		  "automerge":true
	   },
	   {
		  "matchUpdateTypes":[
			 "major"
		  ],
		  "matchCurrentVersion":"!/^0/",
		  "automerge":false
	   }
	]
 }
